<navbar></navbar><h1 class="page-header">Manage Satellites</h1><div class="container-fluid"><div class="panel"><label>Select Satellite:<select ng-model="satmanager.satellite" id="satellite" ng-options="satellite as satellite.name for satellite in satmanager.availableSatellites" class="form-control">		  </select></label></div><div class="panel"><label>All Satellites<ul class="list-group"><li ng-repeat="satellite in satmanager.availableSatellites" class="list-group-item"><strong>{{satellite.name}}<br/><span class="text-muted">{{satellite.id}}</span></strong></li></ul></label></div><div class="panel"><label>Button Examples<ul class="list-group"><button type="button" class="btn btn-primary">Test </button><input type="submit" class="btn btn-primary"/></ul></label></div></div><footer></footer>