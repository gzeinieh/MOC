<navbar></navbar><div class="container-fluid"><div class="container-fluid"><h1 class="page-header">New Command</h1><form ng-submit="tracker.submitCommand(tracker.satellite, tracker.unit, tracker.unitCommand)"><div class="form-group"><label for="satellite">Satellite</label><select ng-model="tracker.satellite" id="satellite" ng-options="satellite as satellite.name for satellite in tracker.availableSatellites" class="form-control"></select></div><div class="form-group"><label for="unit">Unit</label><select type="string" id="unit" ng-model="tracker.unit" ng-options="unit as unit for unit in tracker.satellite.units" class="form-control"></select></div><div class="form-group"><label for="unitCommand">Command</label><select type="string" id="unitCommand" ng-model="tracker.unitCommand" ng-options="command as command for command in tracker.satellite.availableCommands[tracker.unit]" class="form-control"></select></div><div class="form-group"><input type="submit" class="btn btn-primary"/></div></form></div><h1 class="page-header">Assigned Segments</h1><form class="form-inline"><div class="form-group"><label for="selectedGroundstation">Ground Station</label><select ng-model="selectedGroundstation" id="selectedGroundstation" ng-options="station._id as station.name for station in tracker.groundstations" ng-change="tracker.getAssignedSegments(selectedSatellite, selectedGroundstation)" class="form-control"></select></div><div class="form-group"><label for="selectedSatellite">Satellite</label><select ng-model="selectedSatellite" id="selectedSatellite" ng-options="satellite._id as satellite.name for satellite in tracker.availableSatellites" ng-change="tracker.getAssignedSegments(selectedSatellite, selectedGroundstation)" class="form-control"></select></div></form><table class="table table-striped"><thead><th>Start</th><th>End</th><th>Segment ID</th></thead><tbody ng-repeat="pass in tracker.segments"><tr><td>{{ pass.startTime | date:'yyyy-MM-dd HH:mm:ss Z' }}</td><td>{{ pass.endTime | date:'yyyy-MM-dd HH:mm:ss Z'}}</td><td>{{ pass._id }}</td></tr></tbody></table><h1 class="page-header">Unscheduled Commands</h1><table class="table table-striped"><thead><th>Command</th><th>Submitted by</th><th>Actions</th></thead><tbody ng-repeat="command in tracker.unscheduledCommands"><tr><td>{{ command.body }}</td><td>{{ command.User.name}}</td><td><form ng-submit="tracker.scheduleCommand(command, segNum)" class="form-inline"><div class="form-group"><input type="number" ng-model="segNum" placeholder="Segment ID"/></div><div class="form-group"><input type="submit" id="submit" value="Schedule" class="btn btn-primary"/></div></form></td></tr></tbody></table></div><footer></footer>