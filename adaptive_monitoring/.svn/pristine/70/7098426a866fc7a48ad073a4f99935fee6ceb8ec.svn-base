<navbar></navbar><div class="container-fluid"><h1 class="page-header">Queue</h1><table class="table table-striped"><thead><th>Command</th><th>Status</th><th>Submitted By</th><th>Approved By</th><th>Scheduled Station</th><th>Estimated Send</th><th>Segment ID</th><th>Actions</th></thead><tbody ng-repeat="command in queue.commands"><tr><td>{{command.body}}</td><td>{{command.status}}</td><td>{{command.User.name}}</td><td>{{command.approvedBy.name}}</td><td>{{command.scheduledStation}}</td><td>{{command.scheduledSendTime}}</td><td>{{command.SegmentId}}</td><td><a ng-click="queue.unscheduleCommand(command)" class="btn btn-danger">Unschedule</a></td></tr></tbody></table></div><footer></footer>