<navbar></navbar><div class="container-fluid"><h1 class="page-header">New Command</h1><form ng-submit="submitCommand(newCommand)"><div class="form-group"><label for="satellite">Satellite</label><select ng-model="command.satellite" id="satellite" ng-options="satellite._id as satellite.name for satellite in newCommand.availableSatellites" class="form-control"></select></div><div class="form-group"><label for="unit">Unit</label><select type="string" id="unit" ng-model="newCommand.unit" ng-options="unit as unit for unit in newCommand.units" class="form-control"></select></div><div class="form-group"><label for="unitCommand">Command</label><select type="string" id="unitCommand" ng-model="newCommand.body" ng-options="command as command for command in newCommand.unitCommands[newCommand.unit]" class="form-control"></select></div><div class="form-group"><input type="submit" class="btn btn-primary"/></div></form></div><div class="container-fluid"><h1 class="page-header">My Commands</h1><table class="table table-striped"><thead><th>Command</th><th>Status</th><th>Submitted By</th><th>Approved By</th><th>Scheduled Station</th><th>Estimated Send</th></thead><tbody ng-repeat="command in newCommand.myCommands"><tr><td>{{command.body}}</td><td>{{command.status}}</td><td>{{command.User.name}}</td><td>{{command.approvedBy}}</td><td>{{command.scheduledStation}}</td><td>{{command.scheduledSendTime}}</td></tr></tbody></table></div><footer></footer>