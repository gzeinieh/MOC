<navbar></navbar><div class="container-fluid"><h1 class="page-header">Commands to Approve</h1><table class="table table-striped"><thead><th>Command</th><th>Status</th><th>Submitted By</th><th>Actions</th></thead><tbody ng-repeat="command in approval.commands"><tr><td>{{command.body}}</td><td>{{command.status}}</td><td>{{command.User.name}}</td><td><a ng-click="approval.approveCommand(command)" class="btn btn-success">Approve</a><a ng-click="approval.approveCommand(command)" class="btn btn-danger">Deny</a></td></tr></tbody></table></div><footer></footer>